{
    "name": "audriga/jmap-icalendar_vcard",
    "type": "library",
    "description": "Converts between iCalendar and JSCalendar or between vCard and JSContact",
    "keywords": [
        "jmap",
        "groupware"
    ],
    "homepage": "https://github.com/audriga/jmap-php-icalendar_vcard",
    "license": "MIT",
    "authors": [
        {
            "name": "Stanimir Bozhilov",
            "email": "stanimir@audriga.com"
        },
        {
            "name": "Joris Baum",
            "email": "joris@audriga.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "sabre/vobject": ">=4.2",
        "audriga/jmap-openxport": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9",
        "squizlabs/php_codesniffer": ">=3"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "archive": {
        "exclude": [
            "/tests",
            "/build",
            "/README.md"
        ]
    }
}
